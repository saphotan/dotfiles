        {
            "pattern": "msg:[^\\t]+",
            "foreground": "#f500d4",
        }
    ],
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "log.error",
                "settings": {
                    "foreground": "#d30000",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "log.warning",
                "settings": {
                    "foreground": "#e78b00",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "log.info",
                "settings": {
                    "foreground": "#00db1d",
                    "fontStyle": ""
                }
            },
            {
                "scope": "log.debug",
                "settings": {
                    "foreground": "#00a0eb",
                    "fontStyle": ""
                }
            }
        ]
    },
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "workbench.colorTheme": "GitHub Dark Default",
    "workbench.startupEditor": "none",
    "editor.minimap.enabled": false
